{
  "name": "通义千问 AI 助手",
  "version": "1.0.0",
  "description": "支持网络搜索的智能 AI 对话助手，基于阿里云通义千问",
  "author": "gsy",
  "homepage": "https://github.com/gsy44355/web_search_ai_assistant",
  "logo": "logo.png",
  "platform": ["win32", "darwin", "linux"],
  "main": "index.html",
  "preload": "preload.js",
  "features": [
    {
      "code": "ai_chat",
      "explain": "AI 联网对话",
      "cmds": [
        "AI",
        "ai",
        "通义",
        "千问",
        "对话"
      ]
    },
    {
      "code": "ai_ask",
      "explain": "向 AI 提问",
      "cmds": [
        {
          "type": "over",
          "label": "使用AI进行web搜索"
        },
        {
          "type": "regex",
          "label": "向AI提问",
          "match": "/(.*)/",
          "minLength": 2
        }
      ]
    },
    {
      "code": "ai_settings",
      "explain": "AI 设置",
      "cmds": [
        "AI设置",
        "ai设置"
      ]
    }
  ]
}

